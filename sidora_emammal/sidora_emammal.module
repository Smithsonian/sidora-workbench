    }
    else {
       sidora_emammal_create_or_update_for_wi($object);
    }
  }
  else {
    $wi_id = sidora_emammal_wi_get_id($object);
    if (!empty($wi_id)){
      sidora_emammal_debug("Opt-out, but has ID:" . $wi_id . " proceeding to remove");
