/**
 * Direct the creation of all types and handle whatever additional creation of WI
 * objects that needs to happen
 * $type - the WI type
 * $subtype - the WI type to create that is not a direct representation of the base_obj
 *    e.g. location created for a project
 * $base_obj - the Fedora object to use for creation
 * $create_override - normally do not create an object if it already has a WI ID
 *    if TRUE, will create a WI object anyway, and recreate all children
 *    if FALSE, will return the ID of the item, but will continue to create
 *      any children that do not have their WI ID
 */
function sidora_emammal_wi_create($base_obj = NULL, $type = '', $create_override = FALSE){
  // If creating a project or deployment, also create the tree under it.

  if ($type == 'organization'){
    return sidora_emammal_wi_create_organization();
  }

  if ($type == 'project'){
    $existing_wi_id = sidora_emammal_wi_get_id($base_obj);
    sidora_emammal_debug("Create project:" . $type);
    if (empty($existing_wi_id)){
      $project_wi_id = sidora_emammal_wi_create_project($base_obj, $create_override);
      return $project_wi_id;
    }

    // create all the deployments under it
    $subprojects = array();
    sidora_emammal_debug("children");
    $children_concepts = $base_obj->relationships->get(FEDORA_RELS_EXT_URI, 'hasConcept');
    sidora_emammal_debug(json_encode($children_concepts));
    foreach($children_concepts as $child_rel) {
      $to_check_pid = $child_rel['object']['value'];
      $subprojects[] = sidora_obj($to_check_pid);
    }
    foreach($subprojects as $subproject){
      $wi_type = sidora_emammal_get_wi_type($subproject);
      $wi_id = sidora_emammal_wi_get_id($subproject);
      if (empty($wi_id)){
        sidora_emammal_wi_create($subproject, $wi_type, $create_override);
      }
    }
  }

  if ($type == 'subproject'){
    $subproject = $base_obj;
    $wi_id = sidora_emammal_wi_get_id($subproject);
    if (empty($wi_id)){
      $subproject_wi_id = sidora_emammal_wi_create_subproject($base_obj, $create_override);
      return $subproject_wi_id;
    }
    else {
      // Subprojects could have deployments or plots as children
      // Do not create WI objects for plots so follow down to hit its children which are deployments
      $children_concepts = $subproject->relationships->get(FEDORA_RELS_EXT_URI, 'hasConcept');
      foreach($children_concepts as $child_rel) {
        // This could be a deployment or it could be a plot, check the type
        $to_check_pid = $child_rel['object']['value'];
        $check_obj = sidora_obj($to_check_pid);
        $co_wi_type = sidora_emammal_get_wi_type($check_obj);
        if ($co_wi_type == 'deployment'){
          $wi_id = sidora_emammal_wi_get_id($check_obj);
          if (empty($wi_id)){
            sidora_emammal_wi_create($check_obj, 'deployment', $create_override);
          }
        }
        else if ($co_wi_type == 'plot'){
          // This is a plot, check its children for deployments
          $plot_children_concepts = $check_obj->relationships->get(FEDORA_RELS_EXT_URI, 'hasConcept');
          foreach($plot_children_concepts as $plot_child_rel) {
            $to_check_pid = $plot_child_rel['object']['value'];
            $check_obj = sidora_obj($to_check_pid);
            if (sidora_emammal_get_wi_type($check_obj) == 'deployment'){
              $wi_id = sidora_emammal_wi_get_id($check_obj);
              if (empty($wi_id)){
                sidora_emammal_wi_create($check_obj, 'deployment', $create_override);
              }
            }
          }
        }
        else {
          sidora_emammal_debug("Thought this would be a deployment or plot, but was not:" . $check_obj->id . " -> " . $type);
        }
      }
      
    }
  }

  if ($type == 'deployment'){
    $wi_id = sidora_emammal_wi_get_id($base_obj);
    if (empty($wi_id)){
      $deployment_wi_id = sidora_emammal_wi_create_deployment($base_obj, $create_override);
      return $deployment_wi_id;
    }
    else {
      $children_resources = $base_obj->relationships->get(FEDORA_RELS_EXT_URI, 'hasResource');
      foreach($children_resources as $child_rel) {
        $to_check_pid = $child_rel['object']['value'];
        $check_obj = sidora_obj($to_check_pid);
        $co_wi_type = sidora_emammal_get_wi_type($check_obj);
        if ($co_wi_type == 'dataFile'){
          sidora_emammal_wi_create($check_obj, 'dataFile', $create_override);
        }
      }
    }
  }

  if ($type == 'dataFile'){
    $wi_id = sidora_emammal_wi_get_id($base_obj);
    if (empty($wi_id)){
      $wi_id = sidora_emammal_wi_create_datafile($base_obj, $create_override);
    }
    return $wi_id;
  }
  return NULL;
}

