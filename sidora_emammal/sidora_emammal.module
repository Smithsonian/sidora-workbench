  $call_worked = $call_worked && sidora_emammal_wi_api_call("project/" . $proj_id . "/deployment/" .  $deploy_id . "/data-file/" . $wi_id, NULL, 'DELETE');
  sidora_emammal_remove_wi_id($base_obj, $wi_id);
  return $call_worked;
}

