/*
 * This creates the subproject object in WI. It will not create any child objects
 * of a subproject. Does NOT update existing subproject with new data. ONLY used for
 * creation.
 *
 * $base_obj - the Fedora object to use for creation
 * $create_override - normally do not create an object if it already has a WI ID
 *    if TRUE, will create a WI object anyway
 *    if FALSE, will return the ID of the item
 * returns the Wildlife Insights ID of the existing object or newly created object
 */
function sidora_emammal_wi_create_subproject($base_obj, $create_override = FALSE){
  $do_create = $create_override;
  // See if it has a WI id
  $wi_id = sidora_emammal_wi_get_id($base_obj);
  if (empty($wi_id)){
    $do_create = TRUE;
  }
  $proj_id = sidora_emammal_get_project_wi_id($base_obj);
  if (empty($proj_id)){
    // We literally can't force the creation of a subproject in WI, part of the URL is the project WI ID
    sidora_emammal_debug("The project for $base_obj->id is not in WI or does not have opt-in");
    $do_create = FALSE;
  }
  
  sidora_emammal_debug("Create subproject:" . $do_create);
  if ($do_create){
    $update_info = sidora_emammal_wi_subproject_update_strings($base_obj);
    sidora_emammal_debug("update strings:" . json_encode($update_info));
    $subproject_json = $update_info[0]['json'];
    $result = sidora_emammal_wi_api_call("project/" . $proj_id . "/subproject", $subproject_json);
    $subproject_id = sidora_emammal_wi_helper_pull_id($result);
    if (empty($subproject_id)){
      watchdog('Sidora_emammal',t(
        "Tried to create subproject, but did not get subproject's WI ID: %uinfo %result", 
        array('%uinfo' => json_encode($update_info), '%result' => $result)
      ));
    }
    else {
      sidora_emammal_add_wi_id_to_sidora_ds($base_obj, $subproject_id);
    }
    $wi_id = $subproject_id;
  }
  // Now the object is created. Its WI ID is in its SIDORA datastream and
  // in the $wi_id variable
  return $wi_id;
}

/**
 * Returns an array of objects to use to update or create project and location in WI
 * the array looks like this:
 * $returned = array(
 *   [0] => array("wi_type" => "subproject",  "json" => $creation_string),
 * );
 */
function sidora_emammal_wi_subproject_update_strings($base_obj){
  $to_return = array();
  $name = $base_obj->label;
  $content = $base_obj['EAC-CPF']->content;
  if (!empty($content)){
    
  $content = '<?xml version="1.0" encoding="UTF-8"?'.'>' . $content;
  $eac_cpf_to_project_design = '<?xml version="1.0" encoding="UTF-8"?'.'>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:cpf="urn:isbn:1-931666-33-4" version="1.0">
  <xsl:template match="/">
    <xsl:for-each select="//cpf:cpfDescription/cpf:description/cpf:functions/cpf:function">
      <xsl:if test="current()!=\'\'">
        <xsl:if test="cpf:term=\'Project Design\'">
          <xsl:value-of select="cpf:descriptiveNote/cpf:p"/>
        </xsl:if>  
      </xsl:if>  
    </xsl:for-each>
  </xsl:template>
</xsl:stylesheet>
';
    $project_design = sidora_emammal_get_xml_val($content, $eac_cpf_to_project_design);
    $emammal_id = sidora_emammal_get_xml_val($content, NULL, "cpf:recordId");
    $creation_obj = array(
      "name" => $base_obj->label,
      "design" => $project_design,
      "meta" => array(
        "fedora_id" => $base_obj->id,
        "emammal_id" => $emammal_id,
      ),
    );
    $creation_string = json_encode($creation_obj);

  }
  $to_return[] = array("wi_type" => "subproject", "json" => $creation_string);
  return $to_return;
}
