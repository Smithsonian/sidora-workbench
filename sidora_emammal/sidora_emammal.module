/* 
 * This creates the identification outputs in WI for this deployment. It will
 * go to the researcher CSV. 
 * Does NOT update existing identification outputs with new data. ONLY used for
 * creation.
 *
 * $base_obj - the Fedora object to use for creation (deployment)
 * $create_override - normally do not create an object if it already has a WI ID
 *    if TRUE, will create a WI object anyway
 *    if FALSE, will return the ID of the item
 * returns an array of Wildlife Insights ID of the identification outputs
 */
function sidora_emammal_wi_create_sequences_and_identificationoutputs($base_obj, $create_override = FALSE){
  /* What needs to be done here is first to check the CSV for which identifications happened in this 
   * image. Figuring that out requires getting the emammal ID for the image and finding the lines that
   * correspond to it.
   *
   * Finding the CSV based on the image occurs as follows:
   * From the parent of this image, find the child with the title "Researcher Observations"
   * This is made more complicated by the fact that the image could have multiple parents, so
   * use the function to get the image's deployment which checks against isAdministeredBy
   * and then hasConcept relationships.
   */
  $deploy_obj = sidora_emammal_get_deployment_obj($base_obj);
  if (empty($deploy_obj)){
    watchdog('Sidora_emammal',t(
      "Tried to create identification-output for deployment: %dfpid with but did not find the deployment object",
      array('%dpid' => $deploy_obj->id)
    ));
    return NULL;
  }
  $deploy_id = sidora_emammal_get_deployment_wi_id($deploy_obj);
  if (empty($deploy_id)){
    watchdog('Sidora_emammal',t(
      "Tried to create identification-output for deployment: %dfpid with but did not find the deployment WI ID",
      array('%dpid' => $deploy_obj->id)
    ));
    return NULL;
  }
  $proj_id = sidora_emammal_get_project_wi_id($base_obj);
  if (empty($proj_id)){
    watchdog('Sidora_emammal',t(
      "Tried to create identification-output for deployment: %dfpid with but did not find the project",
      array('%dpid' => $deploy_obj->id)
    ));
    return NULL;
  }

  // Go through this object's children to find the Researcher Observations
  $child_res = sidora_concept_resource_listing(
    $deploy_obj->id, 
    array('csv' => array('si:datasetCModel')), 10, 0, 'csv', 'Researcher Observations'
  );
  if (empty($child_res)){
    watchdog('Sidora_emammal',t(
      "Tried to create identification-output for deployment: %dpid but did not find the Researcher Observations file", 
      array('%dpid' => $deploy_obj->id)
    ));
    return NULL;
  }
  $researcher_obs_pid = $child_res[0]['object']['value'];
  $researcher_obs_obj = sidora_obj($researcher_obs_pid);
  $csv_content = $researcher_obs_obj['OBJ']->content;
  $observations = sidora_emammal_pull_observations_for_sequences($csv_content);
  
  sidora_emammal_debug("obs:" . json_encode($observations));
  foreach($observations as $emammal_seq_id => $observations_for_sequence){
    // See if the sequence is already there
    $wi_seq_id = sidora_emammal_wi_get_id($base_obj, 'sequence', $emammal_seq_id);
    $do_create = $create_override;
    if (empty($wi_seq_id)){
      $do_create = TRUE;
    }
    if ($do_create){
      $wi_seq_id = sidora_emammal_wi_create_sequence($deploy_obj, $emammal_seq_id, $create_override);
    }
    
    // See if the observations are already there
    $io_id = sidora_emammal_wi_get_id($base_obj, 'identification_output', $emammal_seq_id);
    $do_create = $create_override;
    if (empty($io_id)){
      $do_create = TRUE;
    }
    if ($do_create){
      $coi_obj = array(
        "blankYn" => true,
        "identificationMethodId" => 1,
        "identifiedObjects" => array(),
      );
      foreach($observations_for_sequence as $obs_line){
        $scientific_name = $obs_line[5];
        $identified_object = array(
          'commonName' => $obs_line[6],
          'relativeAge' => '',
          'sex' => '',
          'markings' => '',
          'individualIdentified' => true,
          'remarks' => $scientific_name,
          'behavior' => '',
          'date' => array(),
          'certainty' => 0,
          'taxonomyId' => 1
        );
        $coi_obj['identifiedObjects'][] = $identified_object;
      }
      $update_info_json = json_encode($coi_obj);
      $result = sidora_emammal_wi_api_call("project/" . $proj_id . '/deployment/' . $deploy_id . '/sequence/' . $wi_seq_id . '/identification-output', $update_info_json);
      sidora_emammal_debug("identification output:" . $result);
      $io_id = sidora_emammal_wi_helper_pull_id($result);
      sidora_emammal_add_wi_id_to_sidora_ds($base_obj, $io_id, 'identification_output', $emammal_seq_id);
    }
  }
  return NULL;
}

/**
 * Returns an array of emammal sequence ids and the content lines for that sequence
 * The array looks something like this, note nested arrays:
 * $returned = array(
 *   ["d18262s1"] => array(
 *      array( "Researcher","d18262","d18262s1","2015-09-18 08:12:27"."2015-09-18 08:14:02","Camera Trapper","Camera Trapper",,,,"1",,,),
 *      array( "Researcher","d18262","d18262s1","2015-09-18 08:12:27"."2015-09-18 08:14:02","Nesotragus moschatus","Suni",,,,"1",,,),
 *   ),
 *   ["d18262s63"] => array(
 *      array( "Researcher","d18262","d18262s63","2015-09-18 08:12:27"."2015-09-18 08:14:02","Camera Trapper","Camera Trapper",,,,"1",,,),
 *   ),
 *   ["d18262s80"] => array(
 *      array( Volunteer,d18262,d18262s80,2015-10-16 10:39:51,2015-10-16 10:40:13,"Camera Trapper","Camera Trapper",,,,1,,,),
 *   ),
 * );
 */
function sidora_emammal_pull_observations_for_sequences($csv_content){
  // There are no line breaks in the eMammal csv values, so we're free to break lines on newlines without worry
  $lines = explode("\n", $csv_content);
  $to_return = array();
  foreach($lines as $line){
    $line_parts = str_getcsv($line);
    if (!empty($line_parts)){
      $emammal_seq_id = $line_parts[2];
      $to_return[$emammal_seq_id][] = $line_parts;
    }
  }
  return $to_return;
}

