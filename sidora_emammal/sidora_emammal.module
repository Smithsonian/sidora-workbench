function sidora_emammal_object_change($pid, $datastream_id){
  $obj = sidora_obj($pid);
  $status = array();
  if (empty($obj)){
    $status['status'] = 'fail';
    $status['message'] = 'object not found'; 
  }
  else {
    $datastream = $obj[$datastream_id];
    if (empty($datastream)){
      $status['status'] = 'fail';
      $status['message'] = 'datastream not found'; 
    }
    else {
      sidora_emammal_create_or_update_for_wi_if_needed($obj, $datastream);
      $status['status'] = 'complete';
    }
  }
  drupal_json_output($status);
  drupal_exit();
}

