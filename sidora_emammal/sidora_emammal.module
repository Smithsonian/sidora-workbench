function sidora_emammal_get_subproject_obj(AbstractObject $object){
  sidora_emammal_debug("Getting subproject for:" . $object->id);
  $wi_infos = sidora_emammal_get_wi_type($object, TRUE);
  sidora_emammal_debug("wi_infos:" . json_encode($wi_infos));
  if (!empty($wi_infos)){
    foreach($wi_infos as $wi_info) {
      sidora_emammal_debug("wi_info:" . json_encode($wi_info));
      $opt_in = FALSE;
      $parent_id = $object->id;
      sidora_emammal_debug("parent_id:" . $parent_id);
      // Walk back up until we hit the project or a subproject
      for($i = 0; $i <= ($wi_info['max_distance_to_project'] - 1); $i++){ 
        $checker_id = $parent_id;
        sidora_emammal_debug("checker_id:" . $checker_id);
        if ($i >= ($wi_info['min_distance_to_project'] - 1)){
          sidora_emammal_debug("past_min_dist_check:" . $checker_id);
          // $checker_id could now hold the pid of the subproject
          $checker_obj = sidora_obj($checker_id);
          $type = sidora_emammal_get_wi_type($checker_obj);
          if ($type == 'subproject'){
            return $checker_obj;
          }
        }
        if ($i < $wi_info['max_distance_to_project']){
          module_load_include('inc', 'sidora', 'includes/utils');
          // use isAdministeredBy if exists
          $parent_id = sidora_get_owning_parent_concept_pid_by_administered_pid($checker_id);
          if (empty($parent_id)){
            $parents = sidora_get_parent_concepts_by_pid($checker_id);
            foreach($parents as $ppid => $parent){
              $parent_id = $ppid;
            }
          }
        }
      }
    }
  }
  sidora_emammal_debug("Couldn't find a subproject for:" . $object->id);
  return FALSE;
}

function sidora_emammal_get_xml_val($xml_content, $my_xsl = NULL, $single_item = NULL){
  if (empty($my_xsl)){
    $my_xsl = '<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:mods="http://www.loc.gov/mods/v3" xmlns:fits="http://hul.harvard.edu/ois/xml/ns/fits/fits_output" xmlns:cpf="urn:isbn:1-931666-33-4" version="1.0">
<xsl:template match="/"><xsl:value-of select="//' . $single_item . '"/></xsl:template>
</xsl:stylesheet>
';
  }
  $xml = new DOMDocument();
  $xml->loadXML($xml_content);

  $xsl = new DOMDocument();
  $xsl->loadXML($my_xsl);

  $proc = new XSLTProcessor();
  $proc->importStyleSheet($xsl);

  $output = trim($proc->transformToDoc($xml)->firstChild->wholeText);
  return $output;
}

