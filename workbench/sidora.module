    $_SESSION['sidora_uploaded_json_config'] = array();
    $_SESSION['sidora_uploaded_json_config']['object_filepath'] = $filepath;
    $_SESSION['sidora_uploaded_json_config']['ignore_settings'] = $ignore_settings;
